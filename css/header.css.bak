/*导航条1*/
	body{
	font-family:Times;
	}
	.container{
			display:flex;
			justify-content:space-between;
			vertical-align:middle;
			border-bottom:1px solid #af9f66;
			margin:40px;
		}
		.container>img{
			width:68px;
			height:65px;
		}
		.nav{
			list-style:none;
			display:flex;
		}
		.nav li{
			margin-left:1.5em;
			line-height:2.5em;
			width:100px;
		}
		.nav li a{
			text-decoration:none;
			color:#999999;
		}
		.nav li a:nth-child(7){
			margin-right:1em;
		}		
		.nav li:nth-child(7) img{
			width:20px;
			height:20px;
				vertical-align:middle;
		}
		.nav li a:hover{
			color:#af9f66;
		}
		/*导航条2*/
		.container2{
		box-sizing:border-box;
		}
		.nav2{
			list-style:none;
			display:flex;
			justify-content:flex-end;
			flex-wrap:no-wrap;
		}
		.nav2 li{
			margin-left:3em;
			
		}
		.nav2:last-child{
			margin-right:40em;
		}
		.nav2>li>a{
		margin:0;
		font-size:15px;
		padding:10px;
		color:#999999;
		text-decoration:none;
		border:1px solid #fff;
		}
		.nav2>li>a:hover{
		color:#af9f66;
		border:1px solid #af9f66;
		}
		/*搜索条*/
	.search{
		background-color:rgba(0,0,0,0.05);
		position:absolute;
		width:200px;
		height:40px;
		border:1px solid #af9f66;
		top:112px;
		right:120px;
	}
	.search>input{
	color:#af9f66;
	outline:0;
	border:0;
	line-height:40px;
	background:0;
	text-indent:20px;
	}